<%= form_for @domain do |f| %>
    <%= f.input :name %>
    <%= f.input :registration %>
    <%= f.input :renewal %>
    <%= f.input :expiration %>
    <%= f.input :dns1 %>
    <%= f.input :dns2 %>
    <%= f.input :epp %>
    <%= f.input :status %>

    <%= f.action :submit, :as => :input %>
<% end %>
